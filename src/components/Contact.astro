<style>
  .contact {
    background: var(--color-warm-white);
  }
  
  .contact-container {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: start;
  }
  
  .contact-info .section-tag {
    text-align: left;
  }
  
  .contact-info h2 {
    text-align: left;
    margin-bottom: 15px;
  }
  
  .contact-info > p {
    margin-bottom: 40px;
  }
  
  .contact-items {
    display: flex;
    flex-direction: column;
    gap: 25px;
  }
  
  .contact-item {
    display: flex;
    align-items: flex-start;
    gap: 20px;
  }
  
  .contact-item-icon {
    width: 50px;
    height: 50px;
    background: var(--color-primary-light);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
  }
  
  .contact-item-icon svg {
    width: 22px;
    height: 22px;
    fill: white;
  }
  
  .contact-item-content h4 {
    font-family: var(--font-body);
    font-size: 1rem;
    font-weight: 600;
    margin-bottom: 5px;
  }
  
  .contact-item-content p {
    font-size: 0.95rem;
    color: var(--color-text-light);
    line-height: 1.6;
  }
  
  .contact-item-content a {
    color: var(--color-primary);
    text-decoration: none;
    transition: var(--transition);
  }
  
  .contact-item-content a:hover {
    color: var(--color-primary-dark);
  }
  
  .social-links {
    display: flex;
    gap: 15px;
    margin-top: 35px;
  }
  
  .social-link {
    width: 48px;
    height: 48px;
    background: var(--color-cream);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: var(--transition);
  }
  
  .social-link:hover {
    background: var(--color-primary);
  }
  
  .social-link:hover svg {
    fill: white;
  }
  
  .social-link svg {
    width: 22px;
    height: 22px;
    fill: var(--color-text);
    transition: var(--transition);
  }
  
  .contact-map {
    border-radius: 20px;
    overflow: hidden;
    box-shadow: var(--shadow-soft);
    height: 450px;
  }
  
  @media (max-width: 1024px) {
    .contact-container {
      grid-template-columns: 1fr;
      gap: 50px;
    }
    
    .contact-map {
      height: 350px;
    }
  }
</style>
